  "source": "pragma solidity ^0.4.17;\n\nimport \"./Escrow.sol\";\n\ncontract EscrowFactory {\n    event EscrowCreated(address newAddress);\n\n    function EscrowFactory() public {}\n\n    function createEscrow(address seller) public payable {\n        address escrowAddress = address(((new Escrow).value(msg.value)(msg.sender, seller)));\n        EscrowCreated(escrowAddress);\n    }\n}",
